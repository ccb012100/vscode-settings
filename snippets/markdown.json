{
  "Date header": {
    "body": [
      "##$0 ==> $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      ""
    ],
    "prefix": "dheader",
    "description": "Insert an H2 date header"
  },
  "Checked checkbox": {
    "prefix": "ChBox",
    "description": "an unchecked checkbox",
    "body": [
      "- [x] "
    ]
  },
  "Unchecked checkbox": {
    "prefix": "UnChBox",
    "description": "an unchecked checkbox",
    "body": [
      "- [ ] "
    ]
  },
  "Source url": {
    "prefix": "source",
    "description": "add source URL for notes",
    "body": "_source_: <$1>$0"
  },
  "link": {
    "prefix": "link",
    "description": "formatted markdown link",
    "body": "[${2:display text}](${1:URL})$0"
  },
  "table": {
    "prefix": "table",
    "description": "new table template",
    "body": [
      "| $1 | $2 |",
      "| - | - |",
      "| $0 | |"
    ]
  },
  "bash-code-block": {
    "description": "add a Bash shell code block",
    "prefix": "bash",
    "body": [
      "```bash",
      "$0",
      "```"
    ]
  },
  "see": {
    "prefix": "see",
    "description": "reference to link for more information",
    "body": "see: [${2:display text}](${1:URL})$0"
  },
  "image": {
    "prefix": "img",
    "description": "add an image tag",
    "body": "![${2:alt text}](/${1:path to image})$0"
  },
  "Mermaid node": {
    "prefix": "mermaid-node",
    "description": "square Mermaid node",
    "body": "${1:Node name}[${2:Node text}]"
  },
  "Mermaid arrow": {
    "prefix": "mermaid-arrow",
    "description": "solid Mermaid arrow",
    "body": "--> "
  },
  "Mermaid arrow with text": {
    "prefix": "mermaid-label-text-arrow",
    "description": "Mermaid arrow with label text on the body",
    "body": "--> | $1 | $0"
  },
  "Mermaid rounded node":{
    "prefix": "mermaid-rounded-node",
    "description": "Mermaid rounded node",
    "body": "${1:Node name}[(${2:Node text})]"
  },
  "Mermaid decision diamond node": {
    "prefix": "mermaid-decision-diamond",
    "description": "Mermaid diamond decision node",
    "body": "${1:Node name}{${2:Node text}}"
  }
}