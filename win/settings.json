{
  // Auto Close Tag
  "auto-close-tag.activationOnLanguage": ["xml", "HTML (Eex)"],
  // bracketPairColorizer
  "bracket-pair-colorizer-2.colors": ["()", "[]", "{}", ["<", "</"], ["<", "/>"], ["Green", "DarkOrchid", "GoldenRod"], "Red"],
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.showBracketsInRuler": true,
  "breadcrumbs.enabled": true,
  // editor
  "editor.codeLens": false,
  "editor.fontFamily": "Fira Code, Hack, mononoki, Consolas, IBM Plex Mono, Ubuntu Mono, Inconsolata",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.letterSpacing": 1,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 60,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.suggest.localityBonus": true,
  "editor.wordWrap": "on",
  // explorer
  "explorer.confirmDragAndDrop": false,
  // files
  "files.associations": {
    "*.vssettings": "xml",
    "*.adoc": "asciidoc",
    "*.DotSettings": "xml"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.js.map": {
      "when": "$(basename)"
    }
  },
  "files.trimTrailingWhitespace": true,
  // Git
  "git.allowForcePush": true,
  "git.confirmForcePush": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autofetch": true,
  "git.defaultCloneDirectory": "C:/code",
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.promptToSaveFilesBeforeCommit": "always",
  "git.rebaseWhenSync": true,
  // Markdown
  "markdown.preview.fontFamily": "IBM Plex Mono",
  // markdownlints
  "markdownlint.config": {
    "MD013": false,
    "MD024": { "siblings_only": true },
    "MD026": false
  },
  // prettier
  "prettier.printWidth": 140,
  "prettier.singleQuote": true,
  "search.showLineNumbers": true,
  // ts-lint
  "tslint.alwaysShowRuleFailuresAsWarnings": true,
  // TypeScript
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  // window
  "window.titleBarStyle": "custom",
  // workbench
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "vscode-icons",
  // Zen Mode
  "zenMode.hideStatusBar": false,
  // YAML
  "yaml.format.enable": true,
  // Markdown
  "[markdown]": {
    "editor.fontFamily": "IBM Plex Mono, 'Consolas', 'Hack', 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.quickSuggestions": true,
    "files.trimTrailingWhitespace": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "tab.activeModifiedBorder": "#FFC300",
    "tab.activeBackground": "#1857a4"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "workbench.sideBar.location": "right",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.external.osxExec": "zsh",
  "window.nativeTabs": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autoStash": true,
  "workbench.colorTheme": "Dracula",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "explorer.incrementalNaming": "smart",
  "editor.cursorSurroundingLines": 5,
  "git.supportCancellation": true,
  "markdown.preview.fontSize": 12,
  "metaGo.decoration.fontSize": 12,
  "workbench.fontAliasing": "auto",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.minimap.size": "fit",
  "workbench.editor.scrollToSwitchTabs": true,
  "window.titleSeparator": "â€£",
  "javascript.preferences.quoteStyle": "single",
  "scm.alwaysShowActions": true,
  "scm.alwaysShowRepositories": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.showWords": false,
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "full",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "git.pruneOnFetch": true,
  "editor.semanticHighlighting.enabled": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "rust-analyzer.checkOnSave.command": "clippy",
  "editor.accessibilitySupport": "off",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "python.languageServer": "Default",
  "rust-analyzer.inlayHints.enable": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "sqlite.recordsPerPage": 100,
  "go.toolsManagement.autoUpdate": true,
  "git.confirmSync": false,
  "markdown.extension.print.theme": "dark",
  "problems.showCurrentInStatus": true,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.enableBell": true,
  "terminal.integrated.env.windows": {
    "MSYSTEM": "MINGW64",
    "MSYS2_PATH_TYPE": "inherit",
    "CHERE_INVOKING": "1"
  },
  "terminal.external.windowsExec": "C:\\\\Program Files\\\\Git\\\\git-bash.exe",
  "terminal.integrated.profiles.windows": {
    "Ubuntu (WSL)": {
      "path": "C:\\\\Windows\\\\System32\\\\wsl.exe",
      "icon": "terminal-ubuntu"
    },
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash",
      "path": "C:\\\\Program Files\\\\Git\\\\git-bash.exe"
    }
  },
  "markdown.preview.typographer": true,
  "remote.WSL.debug": true,
  "redhat.telemetry.enabled": true,
  "markdown.preview.scrollEditorWithPreview": false,
  "terminal.integrated.fontFamily": "Consolas"
}
