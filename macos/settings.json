{
  // Auto Close Tag
  "auto-close-tag.activationOnLanguage": ["xml", "HTML (Eex)"],
  // bracketPairColorizer
  "bracket-pair-colorizer-2.colors": ["()", "[]", "{}", ["<", "</"], ["<", "/>"], ["Green", "DarkOrchid", "GoldenRod"], "Red"],
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.showBracketsInRuler": true,
  "breadcrumbs.enabled": true,
  // editor
  "editor.codeLens": false,
  "editor.fontFamily": "Fira Code, Hack, mononoki, Consolas, IBM Plex Mono, Ubuntu Mono, Inconsolata",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.letterSpacing": 1,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 60,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.suggest.localityBonus": true,
  "editor.wordWrap": "on",
  // explorer
  "explorer.confirmDragAndDrop": false,
  // files
  "files.associations": {
    "*.vssettings": "xml",
    "*.adoc": "asciidoc",
    "*.DotSettings": "xml"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.js.map": {
      "when": "$(basename)"
    }
  },
  "files.trimTrailingWhitespace": true,
  // Git
  "git.allowForcePush": true,
  "git.confirmForcePush": true,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "git.autofetch": true,
  "git.defaultCloneDirectory": "/Users/cbocardo/tools",
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.promptToSaveFilesBeforeCommit": "always",
  "git.rebaseWhenSync": true,
  "git.scanRepositories": ["/Users/cbocardo/repos/"],
  // Markdown
  "markdown.extension.orderedList.autoRenumber": false,
  "markdown.preview.fontFamily": "mononoki",
  // markdownlint
  "markdownlint.config": {
    "MD013": false,
    "MD024": { "siblings_only": true },
    "MD026": false
  },
  // prettier
  "prettier.printWidth": 140,
  "prettier.singleQuote": true,
  "search.showLineNumbers": true,
  // terminal
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.enableBell": true,
  "terminal.integrated.env.windows": {
    "MSYSTEM": "MINGW64",
    "MSYS2_PATH_TYPE": "inherit",
    "CHERE_INVOKING": "1"
  },
  "terminal.external.windowsExec": "C:\\tools\\msys64\\msys2_shell.cmd",
  // "terminal.integrated.cwd": "${fileDirname}",
  "terminal.integrated.fontFamily": "SauceCodePro Nerd Font Mono",
  "terminal.integrated.shell.windows": "C:\\tools\\msys64\\msys2_shell.cmd",
  "terminal.integrated.shellArgs.windows": ["-defterm", "-here", "-no-start"],
  // ts-lint
  "tslint.alwaysShowRuleFailuresAsWarnings": true,
  // TypeScript
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  // window
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  // workbench
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "vscode-icons",
  // Zen Mode
  "zenMode.hideStatusBar": false,
  // YAML
  "yaml.format.enable": true,
  // Markdown
  "[markdown]": {
    "editor.fontFamily": "Ubuntu Mono, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.quickSuggestions": true,
    "files.trimTrailingWhitespace": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "tab.activeModifiedBorder": "#FFC300",
    "tab.activeBackground": "#1857a4"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
