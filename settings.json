    "editor.fontLigatures": false,
    "editor.rulers": [
      150
    ],
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "on",
      "strings": "off"
    },
    "files.trimTrailingWhitespace": false,
    "rewrap.autoWrap.enabled": false,
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.tabSize": 2
  },
  "[plaintext]": {
    "editor.fontLigatures": false,
    "editor.insertSpaces": false,
    "rewrap.autoWrap.enabled": false
